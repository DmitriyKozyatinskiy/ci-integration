  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/node": "^10.11.7",
    "dotenv": "^6.1.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.3"
